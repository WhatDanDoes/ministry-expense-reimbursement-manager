  <header id="page">
    <% if (agent) { %>
      <h1><a href="/"><%= process.env.TITLE %></a></h1>
    <% } else { %>
      <a href="/">
        <section class="splash">
          <p>Ministry</p>
          <p>Expense</p>
          <p>Reimbursement</p>
          <p>Manager</p>
        </section>
      </a>
    <% } %>
    <nav>
      <ul>
        <% if (agent) { %>
          <li><a href="/agent">Profile</a></li>
          <li>|</li>
          <li><a href="/image/<%= agent.getAgentDirectory() %>">Invoices</a></li>
          <li>|</li>
          <li><a href="/logout">Logout</a></li>
        <% } else { %>
          <li><a id="login-link" class="login-button" href="/login">Login</a></li>
        <% } %>
      </ul>
    </nav>
  </header>
